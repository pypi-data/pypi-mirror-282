{
  "openapi": "3.1.0",
  "info": {
    "title": "moapy",
    "description": "Schema for moapy",
    "version": "0.1.0"
  },
  "servers": [
    {
      "url": "https://moa.rpm.kr-dv-midasit.com/backend/function-executor/python-execute/"
    }
  ],
  "paths": {
    "/moapy/project/flared_pier/calc/create_pier": {
      "post": {
        "summary": "Create Pier",
        "operationId": "create pier",
        "parameters": [
          {
            "name": "column_sect_ID",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "title": "Column Sect Id"
            }
          },
          {
            "name": "cap_bot_sect_ID",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "title": "Cap Bot Sect Id"
            }
          },
          {
            "name": "cap_top_sect_ID",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "title": "Cap Top Sect Id"
            }
          },
          {
            "name": "column_matl_ID",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "title": "Column Matl Id"
            }
          },
          {
            "name": "cap_bot_matl_ID",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "title": "Cap Bot Matl Id"
            }
          },
          {
            "name": "cap_top_matl_ID",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "title": "Cap Top Matl Id"
            }
          },
          {
            "name": "start_node_nb",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "title": "Start Node Nb"
            }
          },
          {
            "name": "column_len",
            "in": "query",
            "required": true,
            "schema": {
              "type": "number",
              "title": "Column Len"
            }
          },
          {
            "name": "cap_bot_len",
            "in": "query",
            "required": true,
            "schema": {
              "type": "number",
              "title": "Cap Bot Len"
            }
          },
          {
            "name": "cap_top_len",
            "in": "query",
            "required": true,
            "schema": {
              "type": "number",
              "title": "Cap Top Len"
            }
          },
          {
            "name": "grup_ID",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "title": "Grup Id"
            }
          },
          {
            "name": "bngr_ID",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "title": "Bngr Id"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      }
    }
  }
}